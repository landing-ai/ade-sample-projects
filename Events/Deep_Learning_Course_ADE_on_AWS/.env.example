# LandingAI Configuration
# Get your API key from LandingAI dashboard
VISION_AGENT_API_KEY=your_vision_agent_api_key_here

# AWS Configuration
# Create an IAM user with appropriate permissions for Lambda, S3, Bedrock
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-west-2
# Optional: AWS profile name if using profiles
# AWS_PROFILE=your_profile_name

# S3 Configuration
# Your S3 bucket for document storage
S3_BUCKET=your-s3-bucket-name

# Bedrock Configuration (for medical chatbot)
# Create a knowledge base in AWS Bedrock first
BEDROCK_KB_ID=your_knowledge_base_id
DATA_SOURCE_ID=your_data_source_id
# Bedrock model for the chatbot
BEDROCK_MODEL_ID=us.anthropic.claude-sonnet-4-5-20250929-v1:0

# Lambda Configuration (optional - can use defaults)
# ADE model version
ADE_MODEL=dpt-2-latest
# S3 folder structure
INPUT_FOLDER=input/
OUTPUT_FOLDER=output/
EXTRACTED_FOLDER=extracted/
# Invoice markdown location
INVOICE_MARKDOWN_PATH=output/invoices/
# Set to "true" to reprocess files even if they already exist
FORCE_REPROCESS=false

# Environment
ENVIRONMENT=production

# Optional: Debugging
# Set to "true" for verbose logging
DEBUG=false